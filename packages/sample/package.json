  "scripts": {
    "test": "jest",
    "build:mmorpg": "concurrently -n server,client \"npm run build:server\" \"npm run build:client\"",
    "build:client": "NODE_ENV=production npx webpack",
    "build:server": "npx webpack  --config webpack-server.config.js",
    "build:rpg": "GAME_TYPE=rpg npx webpack",
    "dev:client": "npx webpack -w",
    "dev:server": "npx webpack --config webpack-server.config.js -w",
    "dev:rpg": "GAME_TYPE=rpg npx webpack -w",
    "dev:mmorpg": "concurrently -n server,client \"npm run server-dev\" \"npm run client-dev\"",
    "dev": "npm run dev:mmorpg",
    "dev:nodemon": "nodemon dist/server/index.js",
    "start": "node dist/server"
  },
  "keywords": [],
