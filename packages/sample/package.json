{
  "name": "@rpgjs/sample-game",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "test": "jest",
    "build:mmorpg": "concurrently -n server,client \"npm run build:server\" \"npm run build:client\"",
    "build:client": "NODE_ENV=production RPG_SIDE=client npx webpack",
    "build:server": "NODE_ENV=production RPG_SIDE=server npx webpack",
    "build:rpg": "RPG_TYPE=rpg npx webpack",
    "dev:client": "RPG_SIDE=client npx webpack -w",
    "dev:server": "RPG_SIDE=server npx webpack -w",
    "dev:rpg": "RPG_TYPE=rpg npx webpack -w",
    "dev:mmorpg": "concurrently -n server,client,nodemon \"npm run dev:server\" \"npm run dev:client\" \"npm run dev:nodemon\"",
    "dev": "npm run dev:mmorpg",
    "dev:nodemon": "nodemon dist/server/index.js",
    "start": "node dist/server"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "jest": {
    "transform": {
      "\\.ts$": "ts-jest",
      "\\.tmx$": "tmx-loader"
    },
    "moduleFileExtensions": [
      "ts",
      "js"
    ]
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.6",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "@rpgjs/client": "^3.0.0-alpha",
    "@rpgjs/compiler": "^1.0.0",
    "@rpgjs/database": "^3.0.0-alpha",
    "@rpgjs/default-gui": "^3.0.0-alpha",
    "@rpgjs/server": "^3.0.0-alpha",
    "@rpgjs/standalone": "^3.0.0-alpha",
    "@rpgjs/starter-kit-client": "^3.0.0-alpha",
    "@rpgjs/starter-kit-server": "^3.0.0-alpha",
    "@rpgjs/testing": "^3.0.0-alpha",
    "express": "^4.17.1",
    "socket.io": "^2.3.0",
    "vue": "^2.6.12"
  }
}
